<?xml version="1.0" encoding="UTF-8"?>
<assembly>
	<id>bin</id>
	<formats>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory>target</directory>
			<outputDirectory>/lib</outputDirectory>
			<includes>
				<include>*EMH-light.jar</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/java</directory>
			<outputDirectory>/src</outputDirectory>
			<includes>
				<include>**/EMHExample.java</include>
				<include>org/sercho/masp/models/Context/EMH/MeterReadServlet.java
                </include>
                <include>org/sercho/masp/models/Context/EMH/MeterReadServletStarter.java
                </include>
				<include>org/sercho/masp/models/Context/EMH/CachedGetListRequest.java
				</include>
				<include>org/sercho/masp/models/Context/EMH/ObjectFactory.java
				</include>
				<include>org/sercho/masp/models/Context/EMH/MeterReadings.java
				</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>../Configurations/OrgaSystems</directory>
			<outputDirectory>/config</outputDirectory>
			<includes>
				<include>*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources</directory>
			<outputDirectory>/</outputDirectory>
			<includes>
				<include>EMHExample.bat</include>
				<include>startMeterReadServlet.bat</include>
			</includes>
		</fileSet>
	</fileSets>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/lib</outputDirectory>
			<unpack>false</unpack>
			<scope>runtime</scope>
			<includes>
				<include>org.sercho.masp.models:PropertiesModel</include>
				<include>org.sercho.masp.models:UIModel</include>
				<include>org.sercho.masp.models:MetaMetaModel</include>
				<include>org.sercho.masp:MASPAPIs</include>
				<include>log4j:log4j</include>
				<include>org.openmuc:jsml</include>
				<include>org.eclipse.emf:xmi</include>
				<include>org.eclipse.emf:common</include>
				<include>org.eclipse.emf:ecore</include>
				<include>org.sercho.masp.models:ModelUtilities</include>
				<include>commons-logging:commons-logging</include>
				<include>org.mortbay.jetty:jetty-management</include>
				<include>org.mortbay.jetty:jetty</include>
				<include>org.mortbay.jetty:jetty-util</include>
				<include>javax.servlet:servlet-api</include>
			</includes>
		</dependencySet>
	</dependencySets>
</assembly>